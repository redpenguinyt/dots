[Configuration]
LastActiveMode=THUNAR_RENAMER_MODE_BOTH
LastActiveRenamer=ThunarSbrInsertRenamer

[ThunarSbrCaseRenamer]
Mode=THUNAR_SBR_CASE_RENAMER_MODE_FIRST_UPPER

[ThunarSbrReplaceRenamer]
CaseSensitive=FALSE
Replacement=.
Pattern=-
Regexp=TRUE

[ThunarSbrNumberRenamer]
TextMode=THUNAR_SBR_TEXT_MODE_NTO
Text=-
Mode=THUNAR_SBR_NUMBER_MODE_123
Start=1

[ThunarSbrRemoveRenamer]
StartOffset=1
EndOffset=3
EndOffsetMode=THUNAR_SBR_OFFSET_MODE_LEFT
StartOffsetMode=THUNAR_SBR_OFFSET_MODE_LEFT

[ThunarSbrInsertRenamer]
Offset=0
Text=48-
Mode=THUNAR_SBR_INSERT_MODE_OVERWRITE
OffsetMode=THUNAR_SBR_OFFSET_MODE_LEFT

